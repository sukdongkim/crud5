<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.FlowPane?>

<BorderPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="phone.controller.EmployeeMainController">
   <center>
      <FlowPane hgap="20.0" prefHeight="200.0" prefWidth="200.0" style="-fx-background-color: #4287f5;" vgap="20.0" BorderPane.alignment="CENTER">
         <children>
            <Button mnemonicParsing="false" onAction="#goElectrical" prefHeight="100.0" prefWidth="120.0" text="Electrical Dept." />
            <Button mnemonicParsing="false" onAction="#goMechanical" prefHeight="100.0" prefWidth="120.0" text="Michanical Dept." />
            <Button mnemonicParsing="false" onAction="#onClickAddEmployee" prefHeight="100.0" prefWidth="120.0" text="Add Employee" />
         </children>
         <padding>
            <Insets left="20.0" top="20.0" />
         </padding>
      </FlowPane>
   </center>
</BorderPane>
